      }
    ],
    "triple-equals": [
      true,
      "allow-null-check"
    ],
    "typeof-compare": true,
    "use-isnan": true,
    "variable-name": [
      true,
      "ban-keywords",
      "allow-leading-underscore",
      "allow-trailing-underscore"
    ],
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-module",
      "check-separator",
      "check-type"
    ]
  },
  "defaultSeverity": "error"
}
